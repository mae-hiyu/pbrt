        "__tree": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "future": "cpp",
        "list": "cpp",
        "map": "cpp",
        "set": "cpp",
        "string": "cpp",
        "tuple": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp"
    }
}